{
  "name": "ap_desktop",
  "productName": "AP Desktop",
  "version": "1.3.0",
  "description": "Graphical user interface for QUT's AnalysisPrograms.exe (AP.exe).",
  "main": "src/index.js",
  "scripts": {
    "start": "concurrently --kill-others \"npm run babel:async\" \"electron-forge start\"",
    "package": "electron-packager . AP-Desktop --all --out=out/",
    "make": "electron-forge make",
    "dependencies": "chmod +x ./linux_dependencies.sh && ./linux_dependencies.sh",
    "ap_download": "echo Downloading latest version of AP for Release. This could take up to 15 minutes.&& pwsh -ExecutionPolicy ByPass -File ./script_ap.ps1",
    "install": "npm run ap_download && npm run babel",
    "release": "electron-forge make",
    "publish": "electron-forge publish",
    "test": "npm run babel && mocha tests/test-compiled/tests.js",
    "babel": "npx babel ./src/js/ --out-dir ./src/js-compiled/ && npx babel ./tests/test/ --out-dir ./tests/test-compiled/",
    "babel:async": "npx babel ./src/js/ --out-dir ./src/js-compiled/ -w"
  },
  "keywords": [],
  "author": "Charles Alleman",
  "license": "MIT",
  "config": {
    "forge": {
      "make_targets": {
        "win32": [
          "squirrel"
        ],
        "darwin": [
          "zip"
        ],
        "linux": [
          "snap",
          "deb"
        ]
      },
      "electronPackagerConfig": {
        "packageManager": "npm"
      },
      "electronWinstallerConfig": {
        "name": "ap_desktop"
      },
      "electronInstallerDebian": {},
      "electronInstallerRedhat": {},
      "windowsStoreConfig": {
        "packageName": "",
        "name": "ap_desktop"
      }
    }
  },
  "dependencies": {
    "@babel/polyfill": "^7.4.3",
    "csv-parser": "^2.1.0",
    "electron": "^4.0.1",
    "electron-compile": "^6.4.4",
    "electron-squirrel-startup": "^1.0.0",
    "json2csv": "^4.3.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.3",
    "@babel/core": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "concurrently": "^4.1.0",
    "electron-forge": "^5.2.4",
    "electron-packager": "^13.1.1",
    "electron-prebuilt-compile": "4.0.0",
    "git": "^0.1.5",
    "mocha": "^6.1.3",
    "pwsh": "^0.2.0",
    "spectron": "^5.0.0"
  }
}