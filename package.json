{
  "name": "ap_desktop",
  "productName": "ap_desktop",
  "version": "2.0.0",
  "description": "Graphical user interface for QUT's AnalysisPrograms.exe (AP.exe) software.",
  "main": "src/index.js",
  "scripts": {
    "ap_download": "pwsh -ExecutionPolicy ByPass -File ./scripts/script_ap.ps1",
    "babel:async": "npx babel ./src/js/ --out-dir ./src/js-compiled/ -w",
    "babel": "npx babel ./src/js/ --out-dir ./src/js-compiled/ && npx babel ./tests/test/ --out-dir ./tests/test-compiled/",
    "clean": "rimraf out",
    "dependencies": "chmod +x ./scripts/linux_dependencies.sh && ./scripts/linux_dependencies.sh",
    "install": "npm run ap_download && npm run babel",
    "start": "concurrently --kill-others \"npm run babel:async\" \"electron-forge start\"",
    "test": "npm run babel && mocha tests/test-compiled/tests.js",
    "package": "electron-forge package",
    "make": "npm run clean && electron-forge make",
    "publish": "electron-forge publish",
    "lint": "eslint src --color"
  },
  "keywords": [],
  "author": "Queensland University of Technology",
  "license": "MIT",
  "config": {
    "forge": {
      "make_targets": {
        "win32": [
          "squirrel"
        ],
        "darwin": [
          "zip"
        ],
        "linux": [
          "deb"
        ]
      },
      "electronPackagerConfig": {
        "packageManager": "npm"
      },
      "electronWinstallerConfig": {
        "setupExe": "setup.exe",
        "authors": "Queensland University of Technology"
      },
      "electronInstallerDebian": {
        "options": {
          "name": "ap_desktop",
          "productName": "AP Desktop"
        }
      },
      "electronInstallerRedhat": {},
      "github_repository": {
        "owner": "",
        "name": ""
      },
      "windowsStoreConfig": {
        "packageName": "ap_desktop",
        "name": "ap_desktop"
      }
    }
  },
  "dependencies": {
    "electron-compile": "^6.4.4",
    "electron-squirrel-startup": "^1.0.0",
    "json2csv": "^4.5.0",
    "parse-csv": "^0.2.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.3",
    "@babel/core": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "concurrently": "^4.1.0",
    "electron-forge": "^5.2.4",
    "electron-prebuilt-compile": "4.0.0",
    "eslint": "^3",
    "eslint-config-airbnb": "^15",
    "eslint-plugin-import": "^2",
    "eslint-plugin-jsx-a11y": "^5",
    "eslint-plugin-react": "^7",
    "git": "^0.1.5",
    "mocha": "^6.1.4",
    "pwsh": "^0.2.0",
    "rimraf": "^2.6.3",
    "spectron": "^5.0.0"
  }
}
